#include <iostream>
#include <cstring>

using namespace std;

int main()
{
	int n;
	cout << "输入整数：";
	cin >> n;
	cout << n << endl;
	float n1;
	cout << "输入小数:";
	cin >> n1;
	cout << n1 << endl;
	char ch;
	cout << "输入字符:";
	cin >> ch;
	cout << ch << endl;
	string sh;
	cout << "输入字符串";
	cin >> sh;
	cout << sh << endl;

	return 0;
}
